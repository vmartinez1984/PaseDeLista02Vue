{"ast":null,"code":"import personService from '@/services/PersonsService';\nexport default {\n  name: 'person-vue',\n  data() {\n    return {\n      persons: [],\n      person: {\n        id: 0,\n        name: '',\n        lastName: '',\n        genere: '',\n        birthday: ''\n      },\n      isLoading: false,\n      error: {\n        isError: false,\n        message: ''\n      }\n    };\n  },\n  created() {\n    this.getAllPersons();\n  },\n  methods: {\n    async getAllPersons() {\n      try {\n        this.persons = await personService.getPersonsAsync();\n      } catch (error) {\n        console.log(error);\n        alert('Error :(');\n      }\n      //console.log(persons)\n    },\n\n    async savePerson() {\n      console.log(this.person);\n      if (this.person.id == 0) {\n        this.isLoading = true;\n        this.person.name.disable = true;\n        try {\n          await personService.setPersonAsync(this.person);\n        } catch (error) {\n          console.log(error);\n          alert(\"Ocurrio un error\");\n        }\n        this.isLoading = false;\n      } else {\n        console.log(\"Not implement\");\n      }\n    },\n    resetPerson() {\n      this.person.id = 0;\n      this.person.genere = '';\n      this.person.name = '';\n      this.person.lastName = '';\n      this.isLoading = false;\n    }\n  }\n};","map":{"version":3,"mappings":"AAoGA,OAAOA,aAAY,MAAO,2BAA0B;AAEpD,eAAc;EACVC,IAAI,EAAE,YAAY;EAClBC,IAAI,GAAE;IACF,OAAM;MACJC,OAAO,EAAC,EAAE;MACVC,MAAM,EAAC;QACLC,EAAE,EAAC,CAAC;QACJJ,IAAI,EAAC,EAAE;QACPK,QAAQ,EAAC,EAAE;QACXC,MAAM,EAAC,EAAE;QACTC,QAAQ,EAAE;MACZ,CAAC;MACDC,SAAS,EAAE,KAAK;MAChBC,KAAK,EAAC;QACJC,OAAO,EAAE,KAAK;QACdC,OAAO,EAAE;MACX;IACF;EACJ,CAAC;EACDC,OAAO,GAAE;IACL,IAAI,CAACC,aAAa,EAAC;EACvB,CAAC;EACDC,OAAO,EAAC;IACN,MAAMD,aAAa,GAAE;MACnB,IAAG;QACD,IAAI,CAACX,OAAM,GAAI,MAAMH,aAAa,CAACgB,eAAe,EAAC;MACrD,EAAE,OAAMN,KAAK,EAAC;QACZO,OAAO,CAACC,GAAG,CAACR,KAAK;QACjBS,KAAK,CAAC,UAAU;MAClB;MACA;IACF,CAAC;;IACD,MAAMC,UAAU,GAAE;MAChBH,OAAO