{"ast":null,"code":"export default {\n  name: 'toast-vue',\n  props: ['mensaje', 'titulo', 'mostrar', 'clase'],\n  data() {\n    return {\n      mostrarToast: false\n    };\n  },\n  created() {\n    //console.log(\"mostrar: \", this.mostrar)\n    //console.log(\"mostrarToast: \", this.mostrarToast)\n    this.mostrarToast = this.mostrar;\n  },\n  methods: {\n    ocultarToast() {\n      //console.log(this.mostrarToast)   \n      this.mostrarToast = true;\n      setTimeout(() => {\n        this.mostrarToast = false;\n        //alert(\"Cerrar toast\")\n      }, 5000);\n    }\n  },\n  watch: {\n    mostrar: function (valorNuevo, valorAnterior) {\n      console.log(\"anterior:\", valorAnterior);\n      console.log(\"nuevo: \", valorNuevo);\n      this.ocultarToast();\n    }\n  }\n};","map":{"version":3,"mappings":"AAgBA,eAAc;EACVA,IAAI,EAAE,WAAW;EACjBC,KAAK,EAAC,CAAC,SAAS,EAAC,QAAQ,EAAC,SAAS,EAAC,OAAO,CAAC;EAC5CC,IAAI,GAAE;IACF,OAAO;MACHC,YAAY,EAAE;IAClB;EACJ,CAAC;EACDC,OAAO,GAAE;IACL;IACA;IACA,IAAI,CAACD,YAAW,GAAI,IAAI,CAACE,OAAM;EACnC,CAAC;EACDC,OAAO,EAAC;IACJC,YAAY,GAAE;MACV;MACA,IAAI,CAACJ,YAAW,GAAI,IAAG;MACvBK,UAAU,CAAC,MAAM;QACb,IAAI,CAACL,YAAW,GAAI,KAAI;QACxB;MACJ,CAAC,EAAE,IAAI;IACX;EACJ,CAAC;EACDM,KAAK,EAAC;IACFJ,OAAO,EAAE,UAAUK,UAAU,EAAEC,aAAY,EAAG;MAC1CC,OAAO,CAACC,GAAG,CAAC,WAAW,EAAEF,aAAa;MACtCC,OAAO,CAACC,GAAG,CAAC,SAAS,EAACH,UAAU;MAChC,IAAI,CAACH,YAAY,EAAC;IACtB;EACJ;AACJ","names":["name","props","data","mostrarToast","created","mostrar","methods","ocultarToast","setTimeout","watch","valorNuevo","valorAnterior","console","log"],"sourceRoot":"","sources":["C:\\Users\\vmartinez\\source\\repos\\RollCall\\rollcall-vue\\src\\components\\Toast.vue"],"sourcesContent":["<template>\r\n    <div class=\"toast-container position-fixed bottom-0 end-0 p-3\">\r\n      <div id=\"liveToast\" class=\"toast\" role=\"alert\" aria-live=\"assertive\" aria-atomic=\"true\" :class=\"{'show': mostrarToast}\">\r\n        <div class=\"toast-header\">\r\n          <!-- <img src=\"...\"