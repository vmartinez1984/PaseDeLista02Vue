ase = \"text-primary\"\r\n          this.toast.mostrar = true\r\n        } catch(error){\r\n          this.isError = true\r\n          this.showError = true          \r\n          this.hideToast()\r\n        }        \r\n      },     \r\n      deletePerson(person){\r\n        console.log(person)\r\n      }      \r\n    }    \r\n}\r\n</script>\r\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}