{"ast":null,"code":"export default {\n  name: 'toast-vue',\n  props: ['mensaje', 'titulo', 'mostrar', 'clase'],\n  data() {\n    return {\n      mostrarToast: false\n    };\n  },\n  created() {\n    console.log(\"mostrar: \", this.mostrar);\n    console.log(\"mostrarToast: \", this.mostrarToast);\n  },\n  methods: {\n    ocultarToast() {\n      console.log(this.mostrarToast);\n      setTimeout(() => {\n        //this.mostrarToast = false\n        alert(\"Cerrar toast\");\n      }, 5000);\n    }\n  },\n  watch: {\n    mostrar: function (valorNuevo, valorAnterior) {\n      console.log(\"anterior:\", valorAnterior);\n      console.log(\"nuevo: \", valorNuevo);\n      this.ocultarToast();\n    }\n  }\n};","map":{"version":3,"mappings":"AAgBA,eAAc;EACVA,IAAI,EAAE,WAAW;EACjBC,KAAK,EAAC,CAAC,SAAS,EAAC,QAAQ,EAAC,SAAS,EAAC,OAAO,CAAC;EAC5CC,IAAI,GAAE;IACF,OAAO;MACHC,YAAY,EAAE;IAClB;EACJ,CAAC;EACDC,OAAO,GAAE;IACLC,OAAO,CAACC,GAAG,CAAC,WAAW,EAAE,IAAI,CAACC,OAAO;IACrCF,OAAO,CAACC,GAAG,CAAC,gBAAgB,EAAE,IAAI,CAACH,YAAY;EACnD,CAAC;EACDK,OAAO,EAAC;IACJC,YAAY,GAAE;MACVJ,OAAO,CAACC,GAAG,CAAC,IAAI,CAACH,YAAY;MAC7BO,UAAU,CAAC,MAAM;QACb;QACAC,KAAK,CAAC,cAAc;MACxB,CAAC,EAAE,IAAI;IACX;EACJ,CAAC;EACDC,KAAK,EAAC;IACFL,OAAO,EAAE,UAAUM,UAAU,EAAEC,aAAY,EAAG;MAC1CT,OAAO,CAACC,GAAG,CAAC,WAAW,EAAEQ,aAAa;MACtCT,OAAO,CAACC,GAAG,CAAC,SAAS,EAACO,UAAU;MAChC,IAAI,CAACJ,YAAY,EAAC;IACtB;EACJ;AACJ","names":["name","props","data","mostrarToast","created","console","log","mostrar","methods","ocultarToast","setTimeout","alert","watch","valorNuevo","valorAnterior"],"sourceRoot":"","sources":["C:\\Users\\vmartinez\\source\\repos\\RollCall\\rollcall-vue\\src\\components\\Toast.vue"],"sourcesContent":["<template>\r\n    <div class=\"toast-container position-fixed bottom-0 end-0 p-3\">\r\n      <div id=\"liveToast\" class=\"toast\" role=\"alert\" aria-live=\"assertive\" aria-atomic=\"true\" :class=\"{'show': mostrar}\">\r\n        <div class=\"toast-header\">\r\n          <!-- <img src=\"...\" class=\"rounded me-2\" alt=\"...\"> -->\r\n          <strong class=\"me-auto\" :class=\"clase\">{{ titulo }}</strong>          \r\n          <button type=\"button\" class=\"btn-close\" data-bs-dismiss=\"toast\" aria-label=\"Close\"></button>\r\n        </div>\r\n        <div class=\"toast-body text-info\" :class=\"clase\">\r\n          {{mensaje}}\r\n        </div>\r\n      </div>\r\n    </div>\r\n</template>\r\n<script>\r\n\r\nexport default{    \r\n    name: 'toast-vue',\r\n    props:['mensaje','titulo','mostrar','clase'],\r\n    data(){\r\n        return {\r\n            mostrarToast: false\r\n        }\r\n    },\r\n    created(){\r\n        console.log(\"mostrar: \", this.mostrar)\r\n        console.log(\"mostrarToast: \", this.mostrarToast)\r\n    },\r\n    methods:{\r\n        ocultarToast(){         \r\n            console.log(this.mostrarToast)   \r\n            setTimeout(() => {\r\n                //this.mostrarToast = false\r\n                alert(\"Cerrar toast\")\r\n            }, 5000)      \r\n        }\r\n    },\r\n    watch:{\r\n        mostrar: function (valorNuevo, valorAnterior ){\r\n            console.log(\"anterior:\", valorAnterior)\r\n            console.log(\"nuevo: \",valorNuevo)\r\n            this.ocultarToast()\r\n        }\r\n    }\r\n}\r\n</script>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}